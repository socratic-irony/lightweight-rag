[run]
source = lightweight_rag
omit = 
    */cli.py
    */cli_subprocess.py
    */subprocess_interface.py
    */tests/*
    */test_*.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
